{
  "name": "svg-processor",
  "type": "module",
  "version": "1.0.0",
  "description": "SVG Processor",
  "main": "bin/www.js",
  "scripts": {
    "start": "node -r dotenv/config dist/bin/www",
    "start:dev": "DEBUG=http,convert nodemon --exec babel-node -r dotenv/config src/bin/www",
